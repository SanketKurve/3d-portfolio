{
  "name": "3d-portfolio-mern",
  "version": "1.0.0",
  "description": "MERN Stack Portfolio Website",
  "scripts": {
    "install": "cd frontend && yarn install && cd ../backend-express && npm install",
    "dev": "cd backend-express && npm run dev",
